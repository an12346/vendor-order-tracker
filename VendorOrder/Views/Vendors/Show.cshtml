@using VendorOrder.Models;

<h2>Orders from this vendor:</h2>


@if (Model.Count == 0)
{
  <h3>There are no orders to display</h3>
}
<ul>
  @foreach (Order order in @Model["orders"])
  {
    <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.Title</a></li>


  }
</ul>

<h3><a href='/vendors/@Model["vendor"].Id/orders/new'>Add a new order</a></h3>


